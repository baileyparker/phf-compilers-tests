    x[1].x[1] := 1;
    x[0].y[1] := 1;

    WRITE x[0].x[0];
    WRITE x[0].x[1];
    WRITE x[0].y[0];
    WRITE x[0].y[1];
    WRITE x[1].x[0];
    WRITE x[1].x[1];
    WRITE x[1].y[0];
    WRITE x[1].y[1]
END x.

