PROGRAM x;
TYPE a1 = ARRAY 2 OF INTEGER;
TYPE r2 = RECORD x: a1; y: a1; END;
TYPE a3 = ARRAY 2 OF r2;

VAR x: a3;
BEGIN
    WRITE x[0].x[0];
    WRITE x[0].x[1];
    WRITE x[0].y[0];
    WRITE x[0].y[1];
    WRITE x[1].x[0];
