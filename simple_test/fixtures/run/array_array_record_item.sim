PROGRAM x;
TYPE r1 = RECORD x: INTEGER; y: INTEGER; END;
TYPE a2 = ARRAY 2 OF r1;
TYPE a3 = ARRAY 2 OF a2;

VAR x: a3;
BEGIN
    WRITE x[0][0].x;
    WRITE x[0][0].y;
    WRITE x[0][1].x;
    WRITE x[0][1].y;
    WRITE x[1][0].x;
    WRITE x[1][0].y;
    WRITE x[1][1].x;
    WRITE x[1][1].y;

    x[0][1].x := 1;
    x[1][1].y := 1;

    WRITE x[0][0].x;
    WRITE x[0][0].y;
    WRITE x[0][1].x;
    WRITE x[0][1].y;
    WRITE x[1][0].x;
    WRITE x[1][0].y;
    WRITE x[1][1].x;
    WRITE x[1][1].y
END x.

