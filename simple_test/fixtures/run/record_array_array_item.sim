PROGRAM x;
TYPE a1 = ARRAY 2 OF INTEGER;
TYPE a2 = ARRAY 2 OF a1;
TYPE r3 = RECORD x: a2; y: a2; END;

VAR x: r3;
BEGIN
    WRITE x.x[0][0];
    WRITE x.x[0][1];
    WRITE x.x[1][0];
    WRITE x.x[1][1];
    WRITE x.y[0][0];
    WRITE x.y[0][1];
    WRITE x.y[1][0];
    WRITE x.y[1][1];

    x.x[1][1] := 1;
    x.y[0][1] := 1;

    WRITE x.x[0][0];
    WRITE x.x[0][1];
    WRITE x.x[1][0];
    WRITE x.x[1][1];
    WRITE x.y[0][0];
    WRITE x.y[0][1];
    WRITE x.y[1][0];
    WRITE x.y[1][1]
END x.

