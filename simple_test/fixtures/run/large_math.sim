PROGRAM LargeMath;
VAR x: INTEGER;
VAR y: INTEGER;
VAR z: INTEGER;
VAR a: INTEGER;
VAR b: INTEGER;

BEGIN
  x := 2;
  y := 1;
  z := 2;
  a := 1;
  b := 2;

  WRITE (x + (y + (z + (a + (b + (b + (a + (z + (y + (x + (b + (z * (x)))))))))))));
  WRITE (x + (y + (z + (a + (b + (b + (a + (z + (y + (x + (b + (z DIV (x)))))))))))));
  WRITE (x + (y + (z + (a + (b + (b + (a + (z + (y + (x + (b + (z MOD (x)))))))))))))
END LargeMath.
