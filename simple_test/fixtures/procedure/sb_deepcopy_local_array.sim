PROGRAM X;

PROCEDURE deepCopyArray();
    VAR arr1, arr2: ARRAY 2 OF INTEGER;
BEGIN
    WRITE arr1[0]; (* 0 *)
    WRITE arr1[1]; (* 0 *)
    arr1[0] := 98;
    arr1[1] := 12;
    WRITE arr1[0]; (* 98 *)
    WRITE arr1[1]; (* 12 *)

    WRITE arr2[0]; (* 0 *)
    WRITE arr2[1]; (* 0 *)
    arr2 := arr1;
    WRITE arr2[0]; (* 98 *)
    WRITE arr2[1] (* 12 *)
END deepCopyArray;

BEGIN
    deepCopyArray()
END X.
