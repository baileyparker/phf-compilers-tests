      VAR
          x, y : INTEGER;
      BEGIN
          WRITE x;
          WRITE y
    END bar;

    PROCEDURE foo();
      VAR
          x, y : INTEGER;
      BEGIN
          WRITE x;
          WRITE y;
          bar()
    END foo;
BEGIN
