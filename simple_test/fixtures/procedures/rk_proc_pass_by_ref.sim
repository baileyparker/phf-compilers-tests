PROGRAM X;
TYPE
  arr = ARRAY 10 OF INTEGER;
  rec = RECORD x: INTEGER; y: INTEGER; END;
VAR
  a : arr;
  r : rec;
PROCEDURE passByRef(x: INTEGER; y: arr; z: rec);
  BEGIN
    y[1] := x;
    z.x := x;
    WRITE x;
    WRITE y[0];
    WRITE y[1];
    WRITE y[2];
    WRITE z.x;
    WRITE z.y;
    WRITE x
  END passByRef;
BEGIN
    passByRef(1, a, r);
    WRITE a[0];
    WRITE a[1];
    WRITE a[2];
    WRITE r.x;
    WRITE r.y
END X.
